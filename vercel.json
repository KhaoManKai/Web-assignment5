{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/solutions/projects/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/solutions/projects",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "npm install"
}